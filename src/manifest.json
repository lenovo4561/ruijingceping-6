{
  "package": "com.jsruijingcp.auspicioussign",
  "name": "瑞景测评",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.webview"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug",
    "designWidth": 1080
  },
  "router": {
    "entry": "pages/Launch",
    "pages": {
      "pages/Launch": {
        "component": "index"
      },
      "pages/Demo": {
        "component": "index"
      },
      "pages/DemoDetail": {
        "component": "index"
      },
      "pages/Web": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBar": false,
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Demo": {
        "menu": false
      },
      "pages/DemoDetail": {
        "titleBarText": "详情页"
      }
    }
  },
  "deviceTypeList": ["phone"]
}
